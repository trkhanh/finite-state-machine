{
    "name": "finite-state-machine",
    "version": "0.0.1",
    "description": "An expressive, feature-rich, event-driven JavaScript finite-state machine",
    "scripts": {
      "dev": "webpack --progress --colors --watch --env=dev",
      "prepublish": "npm run build",
