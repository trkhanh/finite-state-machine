      "babel-plugin-add-module-exports": "^0.2.1",
      "babel-preset-es2015": "^6.24.1",
      "chai": "3.4.1",
      "cpx": "^1.5.0",
      "eslint": "1.7.2",
      "eslint-loader": "1.1.0",
      "lite-server": "^2.2.2",
      "mocha": "2.3.4",
      "mocha-logger": "^1.0.4",
      "webpack": "^3.0.0",
      "yargs": "3.32.0"
